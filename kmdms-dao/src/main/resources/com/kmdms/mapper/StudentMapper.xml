<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kmdms.mapper.StudentMapper" >
  <resultMap id="BaseResultMap" type="com.kmdms.pojo.Student" >
    <id column="studentid" property="studentid" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="addStudent" parameterType="com.kmdms.pojo.Student">
  	insert into student (studentid) values (#{studentid});
  </insert>
</mapper>